# Supabase Configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Privy Configuration
# Get your App ID from https://dashboard.privy.io
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here
PRIVY_APP_SECRET=your_privy_app_secret_here

# Thirdweb Configuration (x402 payments)
# Get these from https://thirdweb.com/dashboard/settings/api-keys
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=your_thirdweb_client_id_here
THIRDWEB_SECRET_KEY=your_thirdweb_secret_key_here
THIRDWEB_WALLET_ADDRESS=your_thirdweb_wallet_address_here

# Monad Testnet Configuration
NEXT_PUBLIC_CHAIN_ID=10143
NEXT_PUBLIC_RPC_URL=https://testnet-rpc.monad.xyz
NEXT_PUBLIC_USDC_ADDRESS=0x534b2f3A21130d7a60830c2Df862319e593943A3

# nad.fun Token Deployment
# Network: testnet or mainnet (controls SDK + token URL)
NAD_NETWORK=testnet
MONAD_RPC_URL=https://testnet-rpc.monad.xyz
# Deployer wallet private key (pays ~10 MON per token creation)
NAD_DEPLOYER_PRIVATE_KEY=0x_your_deployer_private_key

# Cost Sink Wallet (deployer wallet - recycles USDC)
COST_SINK_WALLET_ADDRESS=0x94AE63aD0A6aB42e1688CCe578D0DD8b4A2B24e2

# Deployed Contract Addresses (Monad Testnet)
NEXT_PUBLIC_TREASURY_ADDRESS=your_treasury_address_here
NEXT_PUBLIC_AGENT_REGISTRY_ADDRESS=your_agent_registry_address_here
NEXT_PUBLIC_TASK_AUCTION_ADDRESS=your_task_auction_address_here
NEXT_PUBLIC_INTENT_AUCTION_ADDRESS=your_intent_auction_address_here
NEXT_PUBLIC_PARTNERSHIP_ADDRESS=your_partnership_address_here
